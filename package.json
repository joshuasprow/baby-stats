{
  "name": "baby-stats",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "client",
    "functions",
    "lib",
    "models",
    "scripts"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "deploy": "turbo run deploy -- --only functions",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "stage": "turbo run stage",
    "scripts": "turbo run scripts",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "firebase": "9.13.0",
    "turbo": "^1.6.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@8.19.2",
  "volta": {
    "node": "16.18.1"
  }
}
